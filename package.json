  },
  "homepage": "https://github.com/Atyantik/typescript-series#readme",
  "dependencies": {
    "concurrently": "^4.1.0",
    "express": "^4.16.4",
    "nodemon": "^1.18.9",
    "ts-loader": "^5.3.2",
    "typescript": "^3.2.2",
    "webpack": "^4.28.3",
    "webpack-dev-server": "^3.1.14"
  },
